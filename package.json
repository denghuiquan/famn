{
  "name": "famn",
  "version": "0.0.1",
  "description": "famn - angular2 application seed",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "clean": "yarn cache clean && yarn run rimraf -- node_modules dist",
    "clean:dist": "yarn run rimraf -- dist",
    "start:hmr": "NODE_ENV=development HMR=true ts-node server.ts",
    "start:prod": "NODE_ENV=production node ./dist/server.bundle.js",
    "build": "yarn run build:client:dev",
    "prebuild:client:dev": "yarn run clean:dist",
    "prebuild:client:prod": "yarn run clean:dist",
    "build:client:dev": "webpack --config ./client/config/webpack.dev.ts --progress --profile",
    "build:client:prod": "webpack --config ./client/config/webpack.prod.ts --progress --profile --bail",
    "build:server:prod": "webpack --config ./app/webpack/webpack.server.common.ts --progress --profile --bail"
  },
  "author": "Jiale Hu <jiale@implustech.com>",
  "license": "MIT",
  "devDependencies": {
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@ngrx/store-devtools": "3.2.2",
    "@ngrx/store-log-monitor": "3.0.2",
    "@types/body-parser": "0.0.31",
    "@types/bunyan": "0.0.29",
    "@types/chalk": "^0.4.31",
    "@types/compression": "0.0.31",
    "@types/core-js": "^0.9.32",
    "@types/cors": "0.0.31",
    "@types/mongoose": "^4.5.38",
    "@types/node": "^6.0.52",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "6.3.6",
    "awesome-typescript-loader": "2.2.4",
    "codelyzer": "^1.0.0-beta.2",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "postcss-reporter": "1.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "semver": "5.1.0",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.2",
    "ts-node": "^1.3.0",
    "tslint": "3.10.2",
    "tslint-loader": "^2.1.3",
    "tslint-stylish": "2.1.0-beta",
    "typescript": "2.0.7",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.1.0-beta.11",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "~1.0.1"
  },
